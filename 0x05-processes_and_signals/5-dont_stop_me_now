#!/usr/bin/env bash
# Stop the 4-to_infinity_and_beyond process

pids=$(pgrep -f 4-to_infinity_and_beyond)

# If any PIDs are found, kill them
if [ -n "$pids" ]; then
    for pid in $pids; do
        kill "$pid"
        echo "Terminated process with PID $pid"
    done
else
    echo "No running 4-to_infinity_and_beyond process found."
fi
